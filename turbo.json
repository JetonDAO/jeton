{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "check": {
      "dependsOn": ["^check", "//#biome:check"]
    },
    "fix": {
      "dependsOn": ["^fix", "//#biome:fix"]
    },
    "//#biome:check": {},
    "//#biome:fix": {
      "cache": false
    }
  }
}
